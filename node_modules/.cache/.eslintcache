\n  const [errorMessage, setErrorMessage] = useState('')\n  const [toggleLogout, setToggleLogout] = useState(false)\n  const [currentUser, setCurrentUser] = useState({})\n\n  useEffect(()=>{\n    axios\n        .get('https://findmycar-back.onrender.com/cars')\n        .then((response)=>{\n          setCars(response.data);\n        })\n},[])\n\nconst handleNewMake = (event)=>{\n\n  setNewMake(event.target.value);\n}\nconst handleNewModel = (event) => {\n  setNewModel(event.target.value)\n}\nconst handleNewYear = (event) => {\n  setNewYear(event.target.value)\n}\nconst handleNewColor = (event) => {\n  setNewColor(event.target.value)\n}\nconst handleNewImage = (event) => {\n  setNewImage(event.target.value)\n}\nconst handleNewPrice = (event)=>{\n  setNewPrice(event.target.value);\n}\n\nconst handleCreateUser = (userObj) => {\n    axios.post('https://findmycar-back.onrender.com/createaccount', userObj).then((response) => {\n      if(response.data.username){\n        console.log(response);\n        setToggleError(false)\n        setErrorMessage('')\n        setCurrentUser(response.data)\n        handleToggleLogout()\n      } else {\n        setErrorMessage(response.data)\n        setToggleError(true)\n      }\n    })\n  }\n\n  const handleLogin = (userObj) => {\n      console.log(userObj);\n    axios.put('https://findmycar-back.onrender.com/login', userObj).then((response) => {\n      if(response.data.username){\n        console.log(response);\n        setToggleError(false)\n        setErrorMessage('')\n        setCurrentUser(response.data)\n        handleToggleLogout()\n      } else {\n        console.log(response);\n        setToggleError(true)\n        setErrorMessage(response.data)\n      }\n    })\n  }\n\n  const handleLogout = () => {\n    setCurrentUser({})\n    handleToggleLogout()\n  }\n\n  const handleToggleForm = () => {\n    setToggleError(false)\n    if(toggleLogin === true) {\n      setToggleLogin(false)\n    } else {\n      setToggleLogin(true)\n    }\n  }\n\n  const handleToggleLogout = () => {\n    if(toggleLogout) {\n      setToggleLogout(false)\n    } else {\n      setToggleLogout(true)\n    }\n  }\n\n\n\nconst handleNewTodoFormSubmit = (event)=>{\n  axios.post(\n      'https://findmycar-back.onrender.com/cars',\n      {\n          make:newMake,\n          model: newModel,\n          year: newYear,\n          color: newColor,\n          image: newImage,\n          price: newPrice,\n      }\n  ).then(()=>{\n      axios\n          .get('https://findmycar-back.onrender.com/cars')\n          .then((response)=>{\n              setCars(response.data)\n          })\n      })\n}\n\n\n  return (\n    <main>\n    <Nav />\n\n    <div className=\"App\">\n      <div>\n         {toggleLogout ?\n           <button onClick={handleLogout} class='logoutBtn'>Logout</button> :\n           <div class='appFormDiv'>\n             {toggleLogin ?\n             <LoginForm handleLogin={handleLogin} toggleError={toggleError} errorMessage={errorMessage}/>\n             :\n             <NewUserForm handleCreateUser={handleCreateUser} toggleError={toggleError} errorMessage={errorMessage}/>\n             }\n             <button onClick={handleToggleForm} className='button'>{toggleLogin ? 'Create an account?' : 'Already have an account?'}</button>\n            </div>\n         }\n      </div>\n      {currentUser.username ?\n         <div class='loggedInDiv'>\n           <h1>Welcome {currentUser.username}</h1>\n         </div>\n         :\n         null\n       }\n         </div>\n\n   <section className=\"flexColumn\">\n   <h1 className=\"textAlignCenter\">Find My Car</h1>\n     <div className=\"formImgFlex\">\n        <section>\n            <img className=\"formImg\" src=\"https://images.unsplash.com/photo-1532268116505-8c59cc37d2e6?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1164&q=80\"></img>\n        </section>\n        <section className=\"newForm\">\n            <h2 >Setup Car for Sale</h2>\n              <form className=\"formCenter\"onSubmit={handleNewTodoFormSubmit}>\n                 Make: <input type=\"text\" onChange={handleNewMake}/><br/>\n                 Model: <input type=\"text\" onChange={handleNewModel}/><br/>\n                 Year:   <input type=\"number\" onChange={handleNewYear}/><br/>\n                 Color:  <input type=\"text\" onChange={handleNewColor}/><br/>\n                 Image: <input type=\"text\" onChange={handleNewImage}/><br/>\n                 Price: <input type=\"number\" onChange={handleNewPrice}/><br/>\n                 <input id=\"submitNew\" type=\"submit\" value=\"Create new Car\"/>\n              </form>\n\n        </section>\n     </div>\n   </section>\n\n\n\n   <CarList />\n\n\n   <Footer />\n\n   </main>\n  );\n\n}\n\nexport default App;\n","/home/zac/Desktop/Project/Project-3/FindMyCar_Front/src/components/carCard.js",["49","50","51","52"],"\nimport React, { useState, useEffect } from 'react';\nimport axios from 'axios';\n\n\nconst CarCard = (props) => {\n    const {car, handleDelete, editCar} = props;\n    const [newCar, setNewCar] = useState(car);\n    const [showForm, setShowForm] = useState(true);\n    const [showBuy, setShowBuy] = useState(true);\n\n    const handleUpdateCar = (event, field) => {\n        const updatedCar = newCar;\n        const value = event.target.value;\n        if(value != undefined || null || '') {\n            updatedCar[field] = value;\n            setNewCar(updatedCar);\n        } \n    }\n\n    const handleResetCar = () => {\n        setNewCar(car);\n    }\n\n    return (\n        <div className=\"aniCard\" key={car._id}>\n        <img className=\"carImg\" src={car.image} /><br />\n        <div className=\"cardText\">\n            Make: {car.make}<br />\n            Model: {car.model}<br />\n            Year: {car.year}<br />\n            Color: {car.color}<br />\n            Price: ${car.price}<br />\n        </div>\n        <section>\n            {showForm ? (<div className=\"hide\"> Not showing</div>\n            ) : (\n                <div className=\"editCardOverlay\">\n                    <h2>Edit Car</h2>\n                    <form onSubmit={(event) => {\n                        event.preventDefault();\n                        editCar(newCar)\n                        setShowForm(true);\n                    }}>\n                        Make: <input type=\"text\" onChange= {(event) => handleUpdateCar(event, 'make')} placeholder={car.make} /><br />\n                        Model: <input type=\"text\" onChange={(event) => handleUpdateCar(event, 'model')} placeholder={car.model} /><br />\n                        Year: <input type=\"text\" onChange= {(event) => handleUpdateCar(event, 'year')} placeholder={car.year} /><br />\n                        Color: <input type=\"text\" onChange={(event) => handleUpdateCar(event, 'color')} placeholder={car.color} /><br />\n                        Image: <input type=\"text\" onChange={(event) => handleUpdateCar(event, 'image')} placeholder={car.image} /><br />\n                        Price: <input type=\"text\" onChange={(event) => handleUpdateCar(event, 'price')} placeholder={car.price} /><br />\n                        <input id=\"edit\" type=\"submit\" value=\"Submit Edits\" /><br />\n                        <button id=\"editBack\" onClick={() => {\n                            handleResetCar();\n                            showForm === false ? setShowForm(true) : setShowForm(false)\n                        }}>Back</button>\n                    </form>\n                </div>\n\n            )}\n            <div className=\"flexWrapperRowButton\">\n                {showBuy ? (<form onSubmit={(event) => {\n                    //palceholder for Buy Function\n                }}>\n                    <input id=\"buy\" type=\"submit\" value=\"Buy\" onClick={() => {\n                        showBuy === false ? setShowBuy(true) : setShowBuy(false)\n                    }} />\n                </form>) : (<div>Out of Stock</div>)\n\n                }\n                {/* <form onSubmit={(event) => {\n                           //palceholder for Buy Function\n                        }}>\n                              <input id=\"buy\" type=\"submit\" value=\"Buy\" />\n                        </form> */}\n\n\n                {showForm ? (<button key={car._id} id=\"edit\" onClick={() => {\n                    showForm === false ? setShowForm(true) : setShowForm(false)\n                }}>Edit</button>) : (<div></div>)\n\n                }\n                <form onSubmit={(event) => {\n                    event.preventDefault();\n                    handleDelete(car)\n                }}>\n                    <input id=\"delete\" type=\"submit\" value=\"Delete\" />\n                </form>\n            </div>\n\n        </section>\n\n    </div>\n\n    );\n}\n\n\n\nexport default CarCard;\n","/home/zac/Desktop/Project/Project-3/FindMyCar_Front/src/components/nav.js",["53","54","55","56","57","58","59"],"import React, {useState, useEffect} from 'react';\nimport axios from 'axios';\n\n\nconst NavBar = (props) => {\n  \n\n    return(\n        <nav> \n            <div className=\"flexRowNav\">\n                <ul className=\"horiNav\">\n                    <li><a className=\"navLink\" href=\"#\">Used Cars</a></li>\n                    <li><a className=\"navLink\" href=\"#\">New Cars</a></li>\n                    <li><a className=\"navLink\" href=\"#\">Cash Offers</a></li>\n                    <li><a className=\"navLink\" href=\"#\">Find Local Dealers</a></li>\n\n                </ul>\n\n            </div>\n        </nav>\n\n    );\n\n\n}\nexport default NavBar;","/home/zac/Desktop/Project/Project-3/FindMyCar_Front/src/components/footer.js",["60","61"],"import React, {useState, useEffect} from 'react';\n\nconst Footer = () => {\n   return(\n      <footer>\n         <div id=\"Jimmy\">\n            <p>© GYMMEWIN</p>\n            <a href=\"https://www.linkedin.com/in/gymmewin/\"> <img 